<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!--Button normal-->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/liteColorPrimaryDark"/>
            <stroke android:color="@color/black_gone"
                android:width="1dp"/>
            <corners android:radius="4dp"/>
            <padding
                android:left="2dp"
                android:right="2dp"
                android:top="2dp"
                android:bottom="2dp"/>
        </shape>
    </item>

</selector>